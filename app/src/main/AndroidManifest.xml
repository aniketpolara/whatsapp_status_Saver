<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.console.gbversion.whatsscan">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-feature android:name="android.hardware.camera" />

    <queries>
        <package android:name="com.whatsapp" />
        <package android:name="com.whatsapp.w4b" />

        <intent>
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.BROWSABLE" />

            <data android:scheme="https" />
        </intent>
        <intent>
            <action android:name="android.support.customtabs.action.CustomTabsService" />
        </intent>
    </queries>

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <application
        android:allowBackup="false"
        android:allowClearUserData="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.GBVersionForWhatsapp"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.console.gbversion.whatsscan.Application.DirectChat.DirectChatMainActivity"
            android:exported="false" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Profile.SearchProfileMainActivity"
            android:exported="false" />
        <activity
            android:name="com.console.gbversion.whatsscan.Whatsapp.DownloadedStatus_new"
            android:exported="false" />
        <activity
            android:name="com.console.gbversion.whatsscan.Whatsapp.WhatsappstatusActivity"
            android:exported="false" />
        <activity
            android:name="com.console.gbversion.whatsscan.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
        </activity>
        <activity
            android:name=".splashexit.Activity.Splashscreenactivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".splashexit.Activity.Activity_All_Permission"
            android:exported="false" />
        <activity
            android:name=".splashexit.Activity.Backactivity"
            android:exported="false" />
        <activity
            android:name=".splashexit.Activity.Exitactivity"
            android:exported="false" />
        <activity
            android:name=".splashexit.Activity.Firstactivity"
            android:exported="false" />
        <activity
            android:name=".splashexit.Activity.Webactivity"
            android:exported="false" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.AllStatusActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.FirstStartActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.CaptionLanguageActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Adapter.DownloadVideoPlayActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.SocialDownload.DownloadedStatusActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Adapter.DownloadImgDisplayActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.CaptionTypesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.CaptionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.FullCaptionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Adapter.StatusImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Adapter.StatusVideoActivity"
            android:screenOrientation="portrait" />


        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.QRCodeScanActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.GBVersionForWhatsapp"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.ResultActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.GBVersionForWhatsapp" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaToolsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WhatsappWebActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.Message_Recover_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.DirectMessage_Activity"
            android:label="Chatview"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.GBVersionForWhatsapp" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WhatsappCleanerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaShowImageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaVideoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaVideoPlayActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaAudioActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaPlayAudioActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.WaDocsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.SettingsActivity"
            android:screenOrientation="portrait" />

        <service
            android:name="com.console.gbversion.whatsscan.Application.Service.MyService"
            android:enabled="true"
            android:exported="true" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.console.gbversion.whatsscan.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <receiver android:name="com.console.gbversion.whatsscan.Application.Receiver.DeleteAlarmReceiver" />
        <receiver
            android:name="com.console.gbversion.whatsscan.Application.Receiver.AutoStart"
            android:exported="false"
            android:permission="android.permission.RECEIVE_BOOT_COMPLETED">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.console.gbversion.whatsscan.Application.Service.NLServices"
            android:exported="true"
            android:label="@string/app_name"
            android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE">
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService" />
            </intent-filter>
        </service>
        <service
            android:name="com.console.gbversion.whatsscan.Application.Service.MediaListenerService"
            android:enabled="true"
            android:exported="false"
            android:stopWithTask="false" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.FullStatusViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.FullVideoPlayerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.ChatStyleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.StylishTextActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.TextDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.EmojisActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.NumberDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.NumberActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.NameDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.NameActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.EmojiDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.ArtDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.MirrorTextActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.PreviewActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.VideoPlayerActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.console.gbversion.whatsscan.Application.Activity.DirectChatActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>